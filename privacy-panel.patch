+
+section[role="region"] > div.gt-box {
+  top: 0;
+  height: 100%;
+}
+
+.gt-picture {
+  height: calc(50% - 2rem);
+  background: #CEE2E3;
+  overflow: hidden;
+  text-align: center;
+}
+
+.gt-picture img {
+  width: 100%;
+}
+
+.gt-description {
+  overflow: auto;
+  height: calc(50% - 4rem);
+}
+
+.gt-description h2 {
+  color: #00caf2;
+  text-align: center;
+  font-size: 1.8rem;
+  line-height: 2rem;
+  display: block;
+  margin: 0;
+  padding: 1.4rem 1rem 1rem 1rem;
