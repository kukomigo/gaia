+      exports: 'asyncStorage'
+    },
+    'shared/l10n': {
+      exports: 'navigator.mozL10n'
+    }
+  }
+});
+
+(function() {
+  var ppFTU = navigator.mozSettings.createLock()
+    .get('privacy-panel-gt-complete');
+  ppFTU.onsuccess = function() {
