+}
+
+.gt-description p {
+  line-height: 1.5rem;
+  font-size: 1.1rem;
+  text-align: center;
+  color: #585858;
+  padding: 0 2rem;
+}
+
+.gt-action {
+  display: flex;
+  justify-content: center;
+  align-items: center;
+  position: absolute;
+  left: 0;
+  right: 0;
+  bottom: 0;
+  z-index: 10;
+  height: 7rem;
