+
+.custom-location-description p {
+  padding-bottom: 0;
+  padding-top: 0;
+}
+
+/**
+ * Guided tour
+ */
+
+section[data-section="gt"] > div {
+  top: 0;
+  height: 100%;
+}
+
+section[role="region"] > header.gt-header:first-child {
+  background: none;
